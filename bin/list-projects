#!/usr/bin/env bash

# find all directories in the dirs array

dirs=(
  ~/code/sandboxes
  ~/code/plugins.nvim
  ~/code/projects/bleepbloop.git/main/apps
  ~/code/clients/edeka/de.git/feature
  ~/code/clients/edeka/de.git
  ~/code/personal
  ~/code/forks
  ~/code
)

