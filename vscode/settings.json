{
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "files.defaultLanguage": "typescript",
  "editor.fontLigatures": true,
  "window.title": "${rootName}",
  "eslint.packageManager": "yarn",
  "go.toolsManagement.autoUpdate": true,
  "editor.snippetSuggestions": "top",
  "explorer.compactFolders": false,
  "editor.formatOnPaste": false,
  "eslint.alwaysShowStatus": true,
  "explorer.autoReveal": false,
  "editor.matchBrackets": "near",
  "editor.fontSize": 15,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "gitlens.hovers.avatars": false,
  "prettier.tabWidth": 2,
  "emmet.includeLanguages": {
    "twig": "html",
    "nunjucks": "html"
  },
  "editor.accessibilitySupport": "off",
  "emmet.showExpandedAbbreviation": "always",
  "workbench.startupEditor": "newUntitledFile",
  "[scss]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "workbench.tree.indent": 16,
  "editor.guides.indentation": false,
  "prettier.trailingComma": "none",
  "zenMode.fullScreen": false,
  "vim.smartRelativeLine": true,
  "vim.neovimConfigPath": "~/.config/nvim/init.vim",
  "files.exclude": {
    "**\/CVS": true,
    "**\/.DS_Store": true,
    "**\/.hg": true,
    "**\/.svn": true,
    "**\/.git": true
  },
  "editor.lineNumbers": "relative",
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "editor.wordWrapColumn": 100,
  "explorer.confirmDelete": false,
  "gitlens.mode.statusBar.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "vim.useSystemClipboard": true,
  "editor.lineHeight": 1.75,
  "editor.tabSize": 2,
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "eslint.validate": [],
  "terminal.explorerKind": "external",
  "editor.unfoldOnClickAfterEndOfLine": true,
  "diffEditor.renderSideBySide": true,
  "terminal.integrated.fontWeight": "400",
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "git.autofetch": true,
  "vim.neovimUseConfigFile": true,
  "path-intellisense.showHiddenFiles": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressGitVersionWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "terminal.integrated.tabs.location": "left",
  "vim.leader": "<space>",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.fontWeight": 400,
  "git.enableSmartCommit": true,
  "workbench.fontAliasing": "auto",
  "editor.bracketPairColorization.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "k",
      ],
      "commands": [
        {
          "command": "editor.action.marker.prev"
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "j",
      ],
      "commands": [
        {
          "command": "editor.action.marker.next"
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        ".",
      ],
      "commands": [
        {
          "command": "quickOpenCreate.open",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "<space>"
      ],
      "commands": [
        {
          "command": "workbench.action.quickOpen",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "o",
        "o"
      ],
      "commands": [
        {
          "command": "quickOpenCreate.open",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "p",
        "p"
      ],
      "commands": [
        {
          "command": "projectManager.listProjectsNewWindow",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "s",
        "w"
      ],
      "commands": [
        {
          "command": "workbench.action.splitEditorDown",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "f",
        "s"
      ],
      "commands": [
        {
          "command": "workbench.action.files.save",
          "args": []
        }
      ]
    },
    {
      "after": [
        "esc"
      ],
      "before": [
        "<space>",
        "v",
        "w"
      ],
      "commands": [
        {
          "command": "workbench.action.splitEditorRight",
          "args": []
        }
      ]
    }
  ],
  "editor.tabCompletion": "on",
  "editor.stickyScroll.enabled": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.external.osxExec": "kitty.app",
  "extensions.ignoreRecommendations": false,
  "workbench.colorTheme": "Ros√© Pine",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingImportsByDefault": true
  },
  "editor.hover.delay": 100,
  "gitlens.hovers.annotations.changes": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "vim.foldfix": true,
  "vim.operatorPendingModeKeyBindings": [],
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "remote.SSH.remotePlatform": {
    "nocksock.full-t3-app.main.csb": "linux",
    "sandbox.z5grtb.csb": "linux",
    "sandbox.u6nmhf.csb": "linux",
    "sandbox.5whdbw.csb": "linux",
    "sandbox.c5v62o.csb": "linux",
    "sandbox.8hg9u2.csb": "linux",
    "sandbox.n5oogl.csb": "linux"
  },
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "gitlens.advanced.fileHistoryShowAllBranches": true,
  "redhat.telemetry.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "godot_tools.editor_path": "/Applications/Godot.app/Contents/MacOS/Godot",
  "godot_tools.reconnect_attempts": 2,
  "omnisharp.dotnetPath": "/usr/local/share/dotnet/dotnet",
  "omnisharp.monoPath": "/Library/Frameworks/Mono.framework/Versions/Current",
  "omnisharp.sdkPath": "/usr/local/share/dotnet/sdk/7.0.102",
  "settingsSync.ignoredSettings": [
    "omnisharp.dotnetPath",
    "omnisharp.sdkPath",
    "omnisharp.useModernNet"
  ],
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "record-utility-type",
    "typeof"
  ],
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false
  },
  "editor.fontVariations": false,
  "workbench.editor.labelFormat": "medium",
  "cmake.configureOnOpen": false,
  "git.openRepositoryInParentFolders": "always",
  "editor.minimap.enabled": false,
  "telemetry.telemetryLevel": "crash",
  "workbench.sideBar.location": "right",
  "gitlens.currentLine.enabled": false,
  "workbench.activityBar.visible": false,
  "editor.fontFamily": "Iosevka Snock Regular",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
}